
#bar {
    background-color: #121212;
}

#box {
    background-color: #86b5b5;
    padding: 10px;
}

#clock {
    color: #e0e0e0;
    font-size: 22px;
    font-family: comicneue;
}

#workspaces {
    background-color: #262626;
    padding: 5px;
}

#ws-box[state="off"]{
    background-color: #4d4d4d;
    border: 0px;
    border-radius: 5px;
}

#ws-box[state="on"]{
    background-color: #e0e0e0;
    border: 0px;
    border-radius: 5px;
}